<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.TextFlow?>

<GridPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.screen.popups.DialogController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="0.0" prefHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="0.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="0.0" prefHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
   </rowConstraints>
   <children>
      <TextFlow fx:id="textLabel" GridPane.columnSpan="3" />
      <TextField fx:id="textField" GridPane.columnSpan="3" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets top="5.0" />
         </GridPane.margin></TextField>
      <Button fx:id="buttonOK" maxWidth="125.0" minWidth="125.0" mnemonicParsing="false" onAction="#onButtonOKClick" onKeyPressed="#onOKButtonKeyPressed" styleClass="primaryButton" text="%FX_BUTTON_OK" GridPane.rowIndex="6" />
      <Button fx:id="ButtonClick" maxWidth="125.0" minWidth="125.0" mnemonicParsing="false" onAction="#onButtonCancelClick" styleClass="simple-button" text="%FX_BUTTON_CANCEL" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="6" />
      <CheckBox fx:id="anotherOption" mnemonicParsing="false" text="CheckBox" visible="false" GridPane.columnSpan="3" GridPane.halignment="LEFT" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets left="5.0" />
         </GridPane.margin></CheckBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
