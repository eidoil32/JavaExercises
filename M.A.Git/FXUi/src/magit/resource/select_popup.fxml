<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>


<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SelectController">
   <children>
      <TextFlow fx:id="textLabel" prefHeight="50.0" />
      <AnchorPane VBox.vgrow="ALWAYS">
         <children>
            <ChoiceBox fx:id="selectChoiceBox" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding>
      </AnchorPane>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="btnOK" mnemonicParsing="false" onAction="#onButtonOK_Click" text="%FX_BUTTON_OK" />
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#onButtonCancel_Click" text="%FX_BUTTON_CANCEL" />
         </children>
      </HBox>
   </children>
</VBox>
