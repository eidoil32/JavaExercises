<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DialogController">
   <children>
      <AnchorPane prefHeight="125.0" prefWidth="250.0">
         <children>
            <VBox maxHeight="125.0" minHeight="125.0" prefHeight="125.0">
               <children>
                  <AnchorPane maxHeight="90.0" minHeight="90.0" prefHeight="90.0">
                     <children>
                        <TextField fx:id="textField" layoutX="112.0" layoutY="85.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" />
                        <Label fx:id="textLabel" alignment="CENTER" text="Please enter your name" textAlignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                           <padding>
                              <Insets top="10.0" />
                           </padding>
                        </Label>
                     </children>
                  </AnchorPane>
                  <HBox maxHeight="35.0" minHeight="35.0">
                     <children>
                        <AnchorPane minHeight="30.0" minWidth="125.0">
                           <children>
                              <Button fx:id="buttonOK" maxWidth="75.0" minWidth="75.0" mnemonicParsing="false" onAction="#onButtonOKClick" text="%FX_BUTTON_OK" />
                           </children>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </AnchorPane>
                        <AnchorPane prefHeight="30.0" prefWidth="125.0">
                           <children>
                              <Button fx:id="ButtonClick" maxWidth="75.0" minWidth="75.0" mnemonicParsing="false" onAction="#onButtonCancelClick" text="%FX_BUTTON_CANCEL" AnchorPane.rightAnchor="0.0" />
                           </children>
                        </AnchorPane>
                     </children>
                     <padding>
                        <Insets left="5.0" right="5.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</Pane>
