<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.TextFlow?>


<BorderPane minHeight="125.0" minWidth="250.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DialogController">
   <bottom>
      <HBox alignment="CENTER" spacing="80.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="buttonOK" maxWidth="75.0" minWidth="75.0" mnemonicParsing="false" onAction="#onButtonOKClick" text="%FX_BUTTON_OK" />
            <Button fx:id="ButtonClick" maxWidth="75.0" minWidth="75.0" mnemonicParsing="false" onAction="#onButtonCancelClick" text="%FX_BUTTON_CANCEL" />
         </children>
      </HBox>
   </bottom>
   <center>
      <TextField fx:id="textField" BorderPane.alignment="CENTER" />
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <top>
      <TextFlow fx:id="textLabel" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
